<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.boardapp.boardapi.board.mapper.slave.SlaveDatabaseBoardMapper">
    <select id="getAllBoards" resultType="Board">
        SELECT * 
        FROM board
    </select>

    <select id="getByBoardId" resultType="Board">
        SELECT *
        FROM board
        WHERE board_id = #{id}
    </select>
</mapper>
    